{
  "name": "mindful-internet-use-v2",
  "version": "1.0.0",
  "description": "A chrome extension",
  "scripts": {
    "test": "jest --watch ",
    "test-coverage": "jest --coverage ",
    "test-validate": "jest",
    "build": "BABEL_ENV=production NODE_ENV=production ASSET_PATH=/ webpack --config webpack.config.js ",
    "analyze-bundles": "ANALYZE_BUNDLES=true npm run build",
    "start": "node utils/webserver.js",
    "lint": "eslint --ignore-path .gitignore --ext .ts,.tsx . --max-warnings=28",
    "check-types": "tsc",
    "pre-commit": "npm run check-types && lint-staged && npm run test-validate && npm run build",
    "prepare": "husky install",
    "prettier": "prettier --ignore-path .gitignore \"src/**/*.+(json|tsx|ts)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint build test-validate"
  },
  "dependencies": {
    "@headlessui/react": "1.5.0",
    "@heroicons/react": "1.0.6",
    "dayjs": "1.11.1",
    "framer-motion": "6.2.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "3.1.4"
  },
  "devDependencies": {
    "@babel/runtime": "7.17.8",
    "process": "0.11.10",
    "@hot-loader/react-dom": "17.0.2",
    "react-hot-loader": "4.13.0",
    "@babel/core": "7.17.0",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "13.5.0",
    "@types/chrome": "0.0.177",
    "@types/es6-promisify": "^6.0.1",
    "@types/jest": "27.4.1",
    "@types/jest-axe": "3.5.3",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.14",
    "@types/uuid": "8.3.4",
    "@types/webextension-polyfill": "0.8.3",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.16.0",
    "autoprefixer": "10.4.2",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.3",
    "babel-preset-react-app": "10.0.1",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "7.0.0",
    "css-loader": "6.6.0",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.3.0",
    "file-loader": "6.2.0",
    "fs-extra": "10.1.0",
    "html-loader": "3.1.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-axe": "^6.0.0",
    "jest-watch-typeahead": "1.0.0",
    "jest-webextension-mock": "3.7.19",
    "lint-staged": "12.3.7",
    "mini-css-extract-plugin": "2.6.0",
    "node-sass": "6.0.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.6",
    "postcss-loader": "6.2.1",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "0.1.8",
    "sass-loader": "12.6.0",
    "source-map-loader": "3.0.1",
    "style-loader": "3.3.1",
    "tailwindcss": "3.0.19",
    "terser-webpack-plugin": "5.3.1",
    "ts-jest": "^27.1.4",
    "ts-loader": "9.2.8",
    "typescript": "4.5.5",
    "webextension-polyfill": "0.9.0",
    "webpack": "5.72.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.4",
    "webpack-stats-plugin": "1.0.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jest-environment-jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "^@option-ui(.*)$": "<rootDir>/src/options/ui$1",
      "^@ui(.*)$": "<rootDir>/src/ui$1",
      "^@utils(.*)$": "<rootDir>/src/utils$1",
      "^@lib(.*)$": "<rootDir>/src/lib$1",
      "^@types$": "<rootDir>/src/types",
      "^react-dom$": "@hot-loader/react-dom"
    },
    "collectCoverage": false,
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
